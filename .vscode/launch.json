    "version": "0.2.0",
    "configurations": [
        {
            "name": "gdb iamroot",
            "type": "cppdbg",
            "request": "launch",
            "program": "iamroot",
            "args": [
                "-i", "127.0.0.1", "-t", "58001", "-u", "58002", "l:193.136.138.142:59000", "-d", "-p", "3"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}